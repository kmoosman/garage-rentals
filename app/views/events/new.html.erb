<h1 class="header" style="text-align: center; margin: 10px"> Create Event</h1>
<hr/>
<div class="center_me" style="text-align: center">
    <card class="form">
    <form method="post" action="/" enctype="multipart/form-data">
        <section> 
            <label class="block" for="location_name">Event Name</label>
            <input class="block" type="text" name="location_name" id="location_name">
        </section>
        <section> 
            <label class="block" for="start_date">Date</label>
            <input class="block" type="text" name="start_date" id="start_date">
        </section>
        <section> 
            <label class="block" for="end_date">Location</label>
            <input class="block" type="text" name="end_date" id="end_date">
        </section>
        <section class="block" style="text-align: center;">
        <button class="btn btn-primary" type="submit" style="width: 50%; margin-top: 20px; padding: 5px;">Create Event</button>
        </section>
    </form>
    </card>
</div>